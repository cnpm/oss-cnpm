{
  "name": "oss-cnpm",
  "version": "3.0.2",
  "description": "a oss api wraper for cnpmjs.org nfs",
  "keywords": [
    "oss",
    "cnpm"
  ],
  "author": "dead_horse <dead_horse@qq.com>",
  "repository": {
    "type": "git",
    "url": "git://github.com/cnpm/oss-cnpm.git"
  },
  "bugs": {
    "url": "https://github.com/cnpm/oss-cnpm/issues"
  },
  "main": "index.js",
  "files": [
    "index.js"
  ],
  "scripts": {
    "lint": "eslint . --ext .js",
    "lint:fix": "eslint . --ext .js --fix",
    "test": "npm run lint:fix && npm run test-local",
    "test-local": "egg-bin test --full-trace",
    "t": "egg-bin test --changed --full-trace",
    "cov": "egg-bin cov --full-trace",
    "ci": "npm run lint && npm run cov"
  },
  "dependencies": {
    "oss-client": "^1.0.1"
  },
  "devDependencies": {
    "egg-bin": "^4.16.4",
    "eslint": "^8.26.0",
    "eslint-config-egg": "^12.1.0",
    "urllib": "^3.3.1"
  },
  "engines": {
    "node": ">= 16.0.0"
  },
  "ci": {
    "os": "linux",
    "version": "16"
  },
  "license": "MIT"
}

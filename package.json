{
  "name": "oss-cnpm",
  "version": "4.0.1",
  "description": "oss api wrapper for cnpmjs.org nfs",
  "keywords": [
    "oss",
    "cnpm"
  ],
  "author": "dead_horse <dead_horse@qq.com>",
  "repository": {
    "type": "git",
    "url": "git://github.com/cnpm/oss-cnpm.git"
  },
  "bugs": {
    "url": "https://github.com/cnpm/oss-cnpm/issues"
  },
  "main": "index.js",
  "files": [
    "index.js"
  ],
  "scripts": {
    "lint": "eslint . --ext .js",
    "lint:fix": "eslint . --ext .js --fix",
    "test": "npm run lint:fix && npm run test-local",
    "test-local": "egg-bin test --full-trace",
    "t": "egg-bin test --changed --full-trace",
    "cov": "egg-bin cov --full-trace",
    "ci": "npm run lint && npm run cov",
    "contributor": "git-contributor"
  },
  "dependencies": {
    "oss-client": "^2.0.0"
  },
  "devDependencies": {
    "egg-bin": "^5.6.1",
    "eslint": "^8.26.0",
    "eslint-config-egg": "^13.0.0",
    "git-contributor": "^2.1.5",
    "urllib": "^3.3.1"
  },
  "engines": {
    "node": ">= 16.0.0"
  },
  "license": "MIT"
}
